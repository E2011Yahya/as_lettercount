   "source": [
    "\n",
    "\n",
    "def letters(sentence):\n",
    "    dict = {}\n",
    "    for n in sentence:\n",
    "        keys = dict.keys()\n",
    "        if n in keys:\n",
    "            dict[n] += 1\n",
    "        else:\n",
    "            dict[n] = 1\n",
    "    return dict\n",
    "\n",
    "sentence = input(\"write your sentence\")\n",
    "print(letters(sentence))\n"
   ]
